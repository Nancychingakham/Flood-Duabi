      "templateHtml" : "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <style>\n        * {\n            padding: 0;\n            margin: 0;\n        }\n\n        body {\n            height: 100vh;\n            display: flex;\n            justify-content: start;\n            background-color: #efefef;\n            font-family: \"Montserrat\", sans-serif;\n        }\n\n        .row {\n            text-align: center;\n            margin-bottom: 1px;\n        }\n\n        .weather-widget {\n            padding: 2px 0;\n            box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.3);\n            display: flex;\n            flex-flow: column wrap;\n            justify-content: center;\n            justify-items: center;\n            background-color: #2075CD;\n            color: white;\n            width: 50%;\n        }\n\n        .weather-icon {\n            font-size: 2rem;\n            animation: rotation 5s infinite linear;\n        }\n\n        .temp-c {\n            margin-bottom: 1px;\n        }\n\n        .location-country {\n            color: #c2c2c2;\n            font-weight: 30;\n        }\n\n        .weather-forecast {\n            width: 100%;\n        }\n\n        .forecast-item {\n            display: flex;\n            justify-content: space-between;\n            border-bottom: 1px solid #e1e1e1;\n            padding: 50px;\n            transition: all 0.2s;\n        }\n\n        .forecast-item:hover {\n            background-color: #fff;\n            cursor: pointer;\n        }\n\n        .forecast-item p {\n            color: #888;\n            margin-top: 1px;\n        }\n\n        .forecast {\n            text-align: right;\n        }\n\n        @media only screen and (max-width: 600px) {\n            body {\n                flex-flow: column;\n            }\n\n            .weather-widget {\n                width: 100%;\n            }\n\n            .weather-forecast {\n                width: 100%;\n            }\n\n            .temp-c {\n                margin-bottom: 7px;\n            }\n        }\n\n        @keyframes rotation {\n            from {\n                transform: rotate(0deg);\n            }\n            to {\n                transform: rotate(359deg);\n            }\n        }\n    </style>\n    <!-- Include the Montserrat font -->\n    <link href=\"https://fonts.googleapis.com/css2?family=Montserrat:wght@300&display=swap\" rel=\"stylesheet\">\n    <!-- Include the weather icons style if needed -->\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.10/css/weather-icons.min.css\">\n    <title>Weather Widget</title>\n</head>\n<body>\n\n<div class=\"weather-widget\">\n  <div class=\"weather-icon row\">\n    <i class=\"wi wi-day-sunny\"></i>\n  </div>\n  <p class=\"temp-c row\">32C</p>\n  <p class=\"condition row\">Sunny</p>\n  <p class=\"location-name row\">Sarajevo</p>\n  <p class=\"location-country row\">Bosnia and Herzegovina</p>\n</div>\n\n<div class=\"weather-forecast\">\n  <!-- Add the forecast items here -->\n  <!-- ... -->\n  <div class=\"forecast-item\">\n    <div class=\"day\">\n      <p>Tomorrow</p>\n      <p>Aug 6</p>\n    </div>\n    <div class=\"forecast\">\n      <i class=\"wi wi-night-sleet\"></i> 28C\n      <h6>Partly Cloudy</h6>\n    </div>\n  </div>\n\n  <div class=\"forecast-item\">\n    <div class=\"day\">\n      <p>Monday</p>\n      <p>Aug 7</p>\n    </div>\n    <div class=\"forecast\">\n      <i class=\"wi wi-night-sleet\"></i> 28C\n      <p>Partly Cloudy</p>\n    </div>\n  </div>\n\n  <div class=\"forecast-item\">\n    <div class=\"day\">\n      <p>Tuesday</p>\n      <p>Aug 8</p>\n    </div>\n    <div class=\"forecast\">\n      <i class=\"wi wi-day-thunderstorm\"></i> 28C\n      <p>thunderstorms</p>\n    </div>\n  </div>\n\n  <div class=\"forecast-item\">\n    <div class=\"day\">\n      <p>Wednesday</p>\n      <p>Aug 9</p>\n    </div>\n    <div class=\"forecast\">\n      <i class=\"wi wi-day-sunny\"></i> 28C\n      <p>Sunny</p>\n    </div>\n  </div>\n\n  <div class=\"forecast-item\">\n    <div class=\"day\">\n      <p>Thursday</p>\n      <p>Aug 10</p>\n    </div>\n    <div class=\"forecast\">\n      <i class=\"wi wi-night-sleet\"></i> 28C\n      <p>Partly Cloudy</p>\n    </div>\n  </div>\n\n  <div class=\"forecast-item\">\n    <div class=\"day\">\n      <p>Friday</p>\n      <p>Aug 11</p>\n    </div>\n    <div class=\"forecast\">\n      <i class=\"wi wi-night-sleet\"></i> 28C\n      <p>Partly Cloudy</p>\n    </div>\n  </div>\n\n  <div class=\"forecast-item\">\n    <div class=\"day\">\n      <p>Saturday</p>\n      <p>Aug 12</p>\n    </div>\n    <div class=\"forecast\">\n      <i class=\"wi wi-night-sleet\"></i> 28C\n      <p>Partly Cloudy</p>\n    </div>\n  </div>\n\n  <div class=\"forecast-item\">\n    <div class=\"day\">\n      <p>Sunday</p>\n      <p>Aug 13</p>\n    </div>\n    <div class=\"forecast\">\n      <i class=\"wi wi-night-sleet\"></i> 28C\n      <p>Partly Cloudy</p>\n    </div>\n  </div>\n\n</div>\n\n</body>\n</html>\n",
